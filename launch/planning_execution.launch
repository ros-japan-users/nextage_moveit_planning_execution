<?xml version="1.0"?>

<launch>
    <master auto="start"/>
    <include file="$(find nextage_moveit_config)/launch/move_group.launch">
        <arg name="publish_monitored_planning_scene" value="true" />
    </include>
    <include file="$(find nextage_moveit_config)/launch/planning_context.launch">
        <arg name="load_robot_description" value="true"/>
    </include>
    <node pkg="nextage_moveit_planning_execution" type="moveit_command_sender.py" respawn="false" name="moveit_command_sender" output="screen"/>
</launch>
